r\n  //◆イベント\r\n  methods: {\r\n    deleteAllSelected,//外部メソッドを改めて呼び出す\r\n    showDialog() {\r\n      this.dialogVisible = true;\r\n    },\r\n\r\n    async findEmployees() {\r\n      try {\r\n        const response = await axios.get('/employee/search', {\r\n          params: { name: this.searchName },\r\n        });\r\n        this.filteredEmployees = response.data;\r\n      } catch (error) {\r\n        this.$message.error('データの取得に失敗しました'+error);\r\n      }\r\n    },\r\n    //◆明細・参照ボタン\r\n    clickReference(row) {\r\n        alert(row);\r\n    },\r\n   //◆明細・編集ボタン\r\n       clickEdit(row) {\r\n        alert(row);\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = val.map(item => item.id); // 提取所有选中项的 ID\r\n    },\r\n    async loadScreen() {\r\n      try {\r\n        const response = await axios.get('/utils/SearchCompanyList', {\r\n          params: { name: this.searchName },\r\n        });\r\n        this.filteredEmployees = response.data;\r\n      } catch (error) {\r\n        this.$message.error('データの取得に失敗しました'+error);\r\n      }\r\n    }\r\n  },\r\n\r\n  //◆初期化\r\n  mounted() {\r\n    this.filteredEmployees = this.employeeData;\r\n    this.loadScreen(); // 初期データを取得\r\n\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.el-input {\r\n  margin-bottom: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AAwDA,OAAAA,KAAA;AACA,OAAAC,kBAAA;AACA,SAAAC,iBAAA;;AAEA;EAEAC,IAAA;EACAC,UAAA;IAAAH;EAAA;EACA;EACAI,KAAA;IACA;MACAC,UAAA;MACAC,aAAA;MACAC,YAAA;MACAC,UAAA,GACA;QAAAC,IAAA;QAAAP,IAAA;QAAAQ,OAAA;MAAA,GACA;QAAAD,IAAA;QAAAP,IAAA;QAAAQ,OAAA;MAAA,EACA;MACAC,aAAA,GACA;QAAAC,IAAA;QAAAC,KAAA;QAAAC,KAAA;MAAA,GACA;QAAAF,IAAA;QAAAC,KAAA;QAAAC,KAAA;MAAA,GACA;QAAAF,IAAA;QAAAC,KAAA;MAAA,EACA;MACAE,iBAAA;MACAC,aAAA;IACA;EACA;EACA;EACAC,OAAA;IACAhB,iBAAA;IAAA;IACAiB,WAAA;MACA,KAAAF,aAAA;IACA;IAEA,MAAAG,cAAA;MACA;QACA,MAAAC,QAAA,SAAArB,KAAA,CAAAsB,GAAA;UACAC,MAAA;YAAApB,IAAA,OAAAG;UAAA;QACA;QACA,KAAAU,iBAAA,GAAAK,QAAA,CAAAhB,IAAA;MACA,SAAAmB,KAAA;QACA,KAAAC,QAAA,CA