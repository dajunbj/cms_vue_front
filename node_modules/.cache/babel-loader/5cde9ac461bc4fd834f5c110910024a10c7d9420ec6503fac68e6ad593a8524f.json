{"ast":null,"code":"import * as echarts from \"echarts\";\nimport { ref, watchEffect } from \"vue\";\nexport default {\n  name: \"HomeView\",\n  setup() {\n    const activeTab = ref(\"attendance-status\"); // 默认显示考勤状况\n\n    // 图表渲染函数\n    const renderEmployeeStatsChart = () => {\n      const chartDom = document.getElementById(\"employeeStatsChart\");\n      if (!chartDom || chartDom.__echarts__) return;\n      const myChart = echarts.init(chartDom);\n      const option = {\n        title: {\n          text: \"技术者年限分布\"\n        },\n        tooltip: {\n          trigger: \"item\"\n        },\n        legend: {\n          bottom: \"0%\"\n        },\n        series: [{\n          name: \"人数\",\n          type: \"pie\",\n          radius: \"50%\",\n          data: [{\n            value: 10,\n            name: \"1年技术者\"\n          }, {\n            value: 15,\n            name: \"3年技术者\"\n          }, {\n            value: 15,\n            name: \"5年以上技术者\"\n          }, {\n            value: 10,\n            name: \"7年以上技术者\"\n          }]\n        }]\n      };\n      myChart.setOption(option);\n    };\n    const renderAttendanceChart = () => {\n      const chartDom = document.getElementById(\"attendanceChart\");\n      if (!chartDom || chartDom.__echarts__) return;\n      const myChart = echarts.init(chartDom);\n      const option = {\n        title: {\n          text: \"考勤提交情况\"\n        },\n        tooltip: {\n          trigger: \"item\"\n        },\n        legend: {\n          bottom: \"0%\"\n        },\n        series: [{\n          name: \"考勤\",\n          type: \"pie\",\n          radius: \"50%\",\n          data: [{\n            value: 45,\n            name: \"已提交\"\n          }, {\n            value: 5,\n            name: \"未提交\"\n          }]\n        }]\n      };\n      myChart.setOption(option);\n    };\n    const renderContractExpiryChart = () => {\n      const chartDom = document.getElementById(\"contractExpiryChart\");\n      if (!chartDom || chartDom.__echarts__) return;\n      const myChart = echarts.init(chartDom);\n      const option = {\n        title: {\n          text: \"合同即将到期分布\"\n        },\n        tooltip: {\n          trigger: \"item\"\n        },\n        legend: {\n          bottom: \"0%\"\n        },\n        series: [{\n          name: \"人数\",\n          type: \"pie\",\n          radius: \"50%\",\n          data: [{\n            value: 1,\n            name: \"张三 - 客户A\"\n          }, {\n            value: 1,\n            name: \"李四 - 客户B\"\n          }, {\n            value: 1,\n            name: \"王五 - 客户C\"\n          }]\n        }]\n      };\n      myChart.setOption(option);\n    };\n\n    // 根据 Tab 切换初始化图表\n    watchEffect(() => {\n      if (activeTab.value === \"tech-distribution\") {\n        renderEmployeeStatsChart();\n      } else if (activeTab.value === \"attendance-status\") {\n        renderAttendanceChart();\n      } else if (activeTab.value === \"contract-expiry\") {\n        renderContractExpiryChart();\n      }\n    });\n    return {\n      activeTab\n    };\n  },\n  data() {\n    return {\n      techEmployeeList: [{\n        name: \"张三\",\n        joinDate: \"2022年4月\",\n        department: \"开发部\"\n      }, {\n        name: \"李四\",\n        joinDate: \"2021年6月\",\n        department: \"测试部\"\n      }, {\n        name: \"王五\",\n        joinDate: \"2020年8月\",\n        department: \"设计部\"\n      }, {\n        name: \"赵六\",\n        joinDate: \"2019年3月\",\n        department: \"运营部\"\n      }, {\n        name: \"周七\",\n        joinDate: \"2018年11月\",\n        department: \"人事部\"\n      }],\n      notSubmittedList: [{\n        name: \"张三\",\n        department: \"开发部\",\n        contact: \"123-4567-8901\"\n      }, {\n        name: \"李四\",\n        department: \"测试部\",\n        contact: \"123-4567-8902\"\n      }, {\n        name: \"王五\",\n        department: \"设计部\",\n        contact: \"123-4567-8903\"\n      }, {\n        name: \"赵六\",\n        department: \"运营部\",\n        contact: \"123-4567-8904\"\n      }, {\n        name: \"周七\",\n        department: \"人事部\",\n        contact: \"123-4567-8905\"\n      }],\n      expiringContracts: [{\n        name: \"张三\",\n        client: \"客户A\",\n        contractStart: \"2023年1月\",\n        contractEnd: \"2024年1月\"\n      }, {\n        name: \"李四\",\n        client: \"客户B\",\n        contractStart: \"2022年12月\",\n        contractEnd: \"2024年2月\"\n      }, {\n        name: \"王五\",\n        client: \"客户C\",\n        contractStart: \"2023年3月\",\n        contractEnd: \"2024年3月\"\n      }],\n      notifications: [\"年会将于12月25日举行，请大家提前准备。\", \"公司新政策：所有员工考勤需在当月5日提交。\", \"12月公司全员培训，请大家务必参加。\"]\n    };\n  }\n};","map":{"version":3,"names":["echarts","ref","watchEffect","name","setup","activeTab","renderEmployeeStatsChart","chartDom","document","getElementById","__echarts__","myChart","init","option","title","text","tooltip","trigger","legend","bottom","series","type","radius","data","value","setOption","renderAttendanceChart","renderContractExpiryChart","techEmployeeList","joinDate","department","notSubmittedList","contact","expiringContracts","client","contractStart","contractEnd","notifications"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-dashboard\">\r\n    <el-tabs v-model=\"activeTab\" type=\"card\">\r\n      <!-- 技术分布 Tab -->\r\n      <el-tab-pane label=\"技术分布\" name=\"tech-distribution\">\r\n        <div class=\"section\">\r\n          <!-- 左侧技术分布图表 -->\r\n          <div id=\"employeeStatsChart\" class=\"chart\"></div>\r\n\r\n          <!-- 右侧社员详细信息表格 -->\r\n          <div class=\"list\">\r\n            <h4>技术分布详细信息</h4>\r\n            <table class=\"table\">\r\n              <thead>\r\n                <tr>\r\n                  <th>姓名</th>\r\n                  <th>入社年月</th>\r\n                  <th>部门</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"(employee, index) in techEmployeeList\" :key=\"index\">\r\n                  <td>{{ employee.name }}</td>\r\n                  <td>{{ employee.joinDate }}</td>\r\n                  <td>{{ employee.department }}</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </el-tab-pane>\r\n\r\n      <!-- 考勤状况 Tab -->\r\n      <el-tab-pane label=\"考勤状况\" name=\"attendance-status\">\r\n        <div class=\"section\">\r\n          <!-- 左侧考勤图表 -->\r\n          <div id=\"attendanceChart\" class=\"chart\"></div>\r\n\r\n          <!-- 右侧未提交考勤人员一览 -->\r\n          <div class=\"list\">\r\n            <h4>未提交考勤人员一览</h4>\r\n            <table class=\"table\">\r\n              <thead>\r\n                <tr>\r\n                  <th>姓名</th>\r\n                  <th>部门</th>\r\n                  <th>联系方式</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"(person, index) in notSubmittedList\" :key=\"index\">\r\n                  <td>{{ person.name }}</td>\r\n                  <td>{{ person.department }}</td>\r\n                  <td>{{ person.contact }}</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </el-tab-pane>\r\n\r\n      <!-- 合同即将到期社员 Tab -->\r\n      <el-tab-pane label=\"合同即将到期社员\" name=\"contract-expiry\">\r\n        <div class=\"section\">\r\n          <!-- 左侧合同到期概览图表 -->\r\n          <div id=\"contractExpiryChart\" class=\"chart\"></div>\r\n\r\n          <!-- 右侧合同即将到期社员表格 -->\r\n          <div class=\"list\">\r\n            <h4>合同即将到期社员</h4>\r\n            <table class=\"table\">\r\n              <thead>\r\n                <tr>\r\n                  <th>姓名</th>\r\n                  <th>客户名</th>\r\n                  <th>契约开始</th>\r\n                  <th>契约终了</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"(employee, index) in expiringContracts\" :key=\"index\">\r\n                  <td>{{ employee.name }}</td>\r\n                  <td>{{ employee.client }}</td>\r\n                  <td>{{ employee.contractStart }}</td>\r\n                  <td>{{ employee.contractEnd }}</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </el-tab-pane>\r\n\r\n      <!-- 公司通知 Tab -->\r\n      <el-tab-pane label=\"公司通知\" name=\"company-notices\">\r\n        <div class=\"notifications-section\">\r\n          <ul>\r\n            <li v-for=\"(notification, index) in notifications\" :key=\"index\">\r\n              {{ notification }}\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </el-tab-pane>\r\n    </el-tabs>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as echarts from \"echarts\";\r\nimport { ref, watchEffect } from \"vue\";\r\n\r\nexport default {\r\n  name: \"HomeView\",\r\n  setup() {\r\n    const activeTab = ref(\"attendance-status\"); // 默认显示考勤状况\r\n\r\n    // 图表渲染函数\r\n    const renderEmployeeStatsChart = () => {\r\n      const chartDom = document.getElementById(\"employeeStatsChart\");\r\n      if (!chartDom || chartDom.__echarts__) return;\r\n      const myChart = echarts.init(chartDom);\r\n      const option = {\r\n        title: { text: \"技术者年限分布\" },\r\n        tooltip: { trigger: \"item\" },\r\n        legend: { bottom: \"0%\" },\r\n        series: [\r\n          {\r\n            name: \"人数\",\r\n            type: \"pie\",\r\n            radius: \"50%\",\r\n            data: [\r\n              { value: 10, name: \"1年技术者\" },\r\n              { value: 15, name: \"3年技术者\" },\r\n              { value: 15, name: \"5年以上技术者\" },\r\n              { value: 10, name: \"7年以上技术者\" },\r\n            ],\r\n          },\r\n        ],\r\n      };\r\n      myChart.setOption(option);\r\n    };\r\n\r\n    const renderAttendanceChart = () => {\r\n      const chartDom = document.getElementById(\"attendanceChart\");\r\n      if (!chartDom || chartDom.__echarts__) return;\r\n      const myChart = echarts.init(chartDom);\r\n      const option = {\r\n        title: { text: \"考勤提交情况\" },\r\n        tooltip: { trigger: \"item\" },\r\n        legend: { bottom: \"0%\" },\r\n        series: [\r\n          {\r\n            name: \"考勤\",\r\n            type: \"pie\",\r\n            radius: \"50%\",\r\n            data: [\r\n              { value: 45, name: \"已提交\" },\r\n              { value: 5, name: \"未提交\" },\r\n            ],\r\n          },\r\n        ],\r\n      };\r\n      myChart.setOption(option);\r\n    };\r\n\r\n    const renderContractExpiryChart = () => {\r\n      const chartDom = document.getElementById(\"contractExpiryChart\");\r\n      if (!chartDom || chartDom.__echarts__) return;\r\n      const myChart = echarts.init(chartDom);\r\n      const option = {\r\n        title: { text: \"合同即将到期分布\" },\r\n        tooltip: { trigger: \"item\" },\r\n        legend: { bottom: \"0%\" },\r\n        series: [\r\n          {\r\n            name: \"人数\",\r\n            type: \"pie\",\r\n            radius: \"50%\",\r\n            data: [\r\n              { value: 1, name: \"张三 - 客户A\" },\r\n              { value: 1, name: \"李四 - 客户B\" },\r\n              { value: 1, name: \"王五 - 客户C\" },\r\n            ],\r\n          },\r\n        ],\r\n      };\r\n      myChart.setOption(option);\r\n    };\r\n\r\n    // 根据 Tab 切换初始化图表\r\n    watchEffect(() => {\r\n      if (activeTab.value === \"tech-distribution\") {\r\n        renderEmployeeStatsChart();\r\n      } else if (activeTab.value === \"attendance-status\") {\r\n        renderAttendanceChart();\r\n      } else if (activeTab.value === \"contract-expiry\") {\r\n        renderContractExpiryChart();\r\n      }\r\n    });\r\n\r\n    return { activeTab };\r\n  },\r\n  data() {\r\n    return {\r\n      techEmployeeList: [\r\n        { name: \"张三\", joinDate: \"2022年4月\", department: \"开发部\" },\r\n        { name: \"李四\", joinDate: \"2021年6月\", department: \"测试部\" },\r\n